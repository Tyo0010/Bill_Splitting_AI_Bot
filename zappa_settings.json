{
    "main": { 
        "app_function": "app.lambda_handler",
        "project_name": "bill-splitting-bot",
        "runtime": "python3.11",
        "s3_bucket": "zappa-bill-splitter-unique-bucket",
        "aws_region": "ap-southeast-1",
        "timeout_seconds": 60, 
        "memory_size": 256,   
        "keep_warm": false,
        "aws_environment_variables": {

            "BOT_TOKEN": "",
            "GEMINI_API_KEY": "",
            "BOT_USERNAME": "",
            "WEBHOOK_URL": ""
         },
         "exclude": [
            "*.pyc",
            "__pycache__",
            ".git",
            "tests",
            "*.log",
            "venv",
            "*.DS_Store"
        ],
        "touch": false 
        
    }
}